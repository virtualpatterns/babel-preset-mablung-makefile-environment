{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/**/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "c8": {
    "all": true,
    "exclude": [],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    }
  },
  "eslintIgnore": [
    "source/sandbox",
    "source/test"
  ],
  "exports": {
    ".": "./release/index.cjs",
    "./package.json": "./package.json"
  },
  "files": [
    "release/index.cjs"
  ],
